{
  "name": "parcoord-es",
  "version": "1.0.1",
  "description": "ES6 module of Parallel Coordinates, based on d3 v4",
  "main": "dist/parcoords.js",
  "module": "dist/parcoords.mjs",
  "jsnext:main": "dist/parcoords.mjs",
  "style": "dist/parcoords.css",
  "scripts": {
    "_prebuild": "eslint src test",
    "build": "rollup -c",
    "dev": "rollup -c rollup.config.dev.js -w",
    "watch": "rollup -c -w",
    "pretest": "npm run build",
    "test:watch": "mocha --reporter min --watch",
    "test": "mocha",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "prepublish": "npm test"
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "d3-array": "^1.2.0",
    "d3-axis": "^1.0.8",
    "d3-brush": "^1.0.4",
    "d3-collection": "^1.0.4",
    "d3-dispatch": "^1.0.3",
    "d3-drag": "^1.1.0",
    "d3-interpolate": "^1.1.5",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "d3-transition": "^1.1.0",
    "lodash-es": "^4.17.4",
    "requestanimationframe": "0.0.23"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^4.1.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.4.2",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-livereload": "^0.4.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.3",
    "rollup-plugin-serve": "^0.3.0",
    "rollup-watch": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigFatDog/parcoords-es.git"
  },
  "keywords": [
    "es6",
    "d3",
    "v4",
    "ParCoords",
    "visualization",
    "parallel coordinates"
  ],
  "author": {
    "name": "Xing Yun"
  },
  "license": "MIT",
  "bugs": {
    "url": "hhttps://github.com/BigFatDog/parcoords-es/issues"
  }
